
@{
    ViewData["Title"] = "Features";
    Layout = "~/Views/_Layout.cshtml";
}
@model Character

<h1>Features/Proficiencies</h1>

<form method="post">
    <p>
        <label asp-for="Features.FeatureField.featureName">Feature Name:</label>
        <input asp-for="Features.FeatureField.featureName" />
    </p>
    <p>
        <label asp-for="Features.FeatureField.featureDescription">Feature Description:</label>
        <input asp-for="Features.FeatureField.featureDescription" />
    </p>
    <p>
        <label asp-for="ProficiencyField">Proficiency</label>
        <input asp-for="ProficiencyField"/>
    </p>
    <p><button type="submit">Create</button></p>
</form>

